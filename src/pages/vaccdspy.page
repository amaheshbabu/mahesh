<apex:page standardController="Account" recordSetVar="rec">
  <apex:form >
  <apex:pageBlock >
  <apex:pageBlockTable value="{!rec}" var="a">
  <apex:column value="{!a.Name}" />
  <apex:column value="{!a.Phone}"/>
  <apex:column value="{!a.Fax}"/>
  <apex:column value="{!a.Rating}"/>
  </apex:pageBlockTable>
  </apex:pageBlock>
  </apex:form>
</apex:page>